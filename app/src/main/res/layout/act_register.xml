<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ui.activity.RegisterActivity">

    <EditText
            android:layout_width="0dp"
            android:layout_height="50dp"
            android:inputType="textPersonName"
            android:text="Name"
            android:ems="10"
            tools:layout_editor_absoluteY="160dp" tools:layout_editor_absoluteX="98dp" android:id="@+id/edit_Nome"/>
    <EditText
            android:layout_width="0dp"
            android:layout_height="50dp"
            android:inputType="textPersonName"
            android:text="Name"
            android:ems="10"
            tools:layout_editor_absoluteY="225dp" tools:layout_editor_absoluteX="98dp" android:id="@+id/edit_email"/>
    <EditText
            android:layout_width="0dp"
            android:layout_height="50dp"
            android:inputType="textPersonName"
            android:text="Name"
            android:ems="10"
            tools:layout_editor_absoluteY="281dp" tools:layout_editor_absoluteX="98dp" android:id="@+id/edit_password"/>
    <Button
            android:text="Button"
            android:layout_width="0dp"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="341dp"
            tools:layout_editor_absoluteX="161dp" android:id="@+id/btn_registrar"/>
</androidx.constraintlayout.widget.ConstraintLayout>